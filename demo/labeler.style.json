{
  "version": 8,
  "sprite": "http://localhost:9091/out/sprites-labeler",
  "sources": {
    "basemap_default": {
      "type": "vector",
      "tiles": [
        "https://storage.googleapis.com/public-tiles/basemap/default/{z}/{x}/{y}.pbf"
      ],
      "maxzoom": 9
    },
    "geojson": {
      "type": "geojson"
    }
  },
  "layers": [
      {
          "id": "bg",
          "type": "background",
          "paint": {
              "background-color": "#002357"
          }
      },
      {
          "id": "countries",
          "type": "fill",
          "source": "basemap_default",
          "source-layer": "countries",
          "paint": {
              "fill-color": "#274777"
          }
      },
      {
        "id": "arrow outline",
        "type": "symbol",
        "source": "geojson",
        "layout": {
          "icon-allow-overlap": true,
          "icon-image": "arrow-outer",
          "icon-size": 1
        },
        "paint": {
          "icon-color": [
            "case",
            ["==", ["get", "status"], "fishing"], "#FFD714",
            ["==", ["get", "status"], "selected"], "#FFF",
            ["==", ["get", "status"], "unlabeled"], "#8091AB",
              "red"
          ]
        }
      },
      {
        "id": "arrow fill",
        "type": "symbol",
        "source": "geojson",
        "layout": {
          "icon-allow-overlap": true,
          "icon-image": "arrow-inner",
          "icon-size": 1
        },
        "paint": {
          "icon-color": [
            "interpolate",
            ["linear"],
            ["get", "value"],
            0, "#FF6B6B",
            6, "#F55582",
            12, "#DF499C",
            18, "#BB4AB4",
            24, "#8652C6",
            30, "#185AD0"
          ]
        }
      }
  ]
}